Simulation/*.pidb.meta
Simulation/*.pdb.meta
Simulation/*.mdb.meta

# Unity3D generated file on crash reports
Simulation/sysinfo.txt

# Builds
Simulation/*.apk
Simulation/*.aab
Simulation/*.unitypackage
Simulation/*.app

# Crashlytics generated file
Simulation/crashlytics-build.properties

